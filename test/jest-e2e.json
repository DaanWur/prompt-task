{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@app/auth": "<rootDir>/../libs/auth/src",
    "@app/exceptions/(.*)": "<rootDir>/../libs/exceptions/src/$1",
    "@app/exceptions": "<rootDir>/../libs/exceptions/src",
    "@app/prisma/(.*)": "<rootDir>/../libs/prisma/src/$1",
    "@app/prisma": "<rootDir>/../libs/prisma/src",
    "@app/cache/(.*)": "<rootDir>/../libs/cache/src/$1",
    "@app/cache": "<rootDir>/../libs/cache/src"
  }
}